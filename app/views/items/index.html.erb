<h1> All items </h1>
<table>
  <tr>
    <th>Title</th>
    <th>Cost</th>
    <th></th>
  </tr>
  <% @items.each do |item| %>
    <tr>
      <td><%= item.title %></td>
      <td><%= item.cost %></td>
      <td><%= link_to 'Show', item_path(item) %></td>
    </tr>
  <% end %>
</table>
